{
	"name": "@kopexa/react-utils",
	"version": "2.0.6",
	"description": "A set of utilities for react on client side",
	"keywords": [
		"react-utils"
	],
	"author": "Kopexa <hello@kopexa.com>",
	"homepage": "https://kopexa.com",
	"license": "Apache-2.0",
	"main": "src/index.ts",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kopexa-grc/sight.git",
		"directory": "packages/utilities/react-utils"
	},
	"bugs": {
		"url": "https://github.com/kopexa-grc/sight/issues"
	},
	"scripts": {
		"build": "tsup src --dts",
		"build:fast": "tsup src",
		"dev": "pnpm build:fast --watch",
		"clean": "rimraf dist .turbo",
		"typecheck": "tsc --noEmit",
		"prepack": "clean-package",
		"postpack": "clean-package restore"
	},
	"peerDependencies": {
		"react": ">=19.0.0-rc.0"
	},
	"dependencies": {
		"@kopexa/shared-utils": "workspace:*"
	},
	"devDependencies": {
		"clean-package": "2.2.0",
		"react": "^19.1.1"
	},
	"clean-package": "../../../clean-package.config.json"
}
